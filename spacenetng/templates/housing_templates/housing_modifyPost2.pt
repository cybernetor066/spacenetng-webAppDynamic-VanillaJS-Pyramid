        <!-- Landing Page Content -->
        <div id="create-new-post" class="grid_12">

            <form action="${request.current_route_url()}" method="POST" enctype="multipart/form-data">

            <input type="hidden" value=${private_UUID} id="privateUUID" name="housing_update_privateUUID"/>

            <p id="regionSel">Please select target region:</p>
            <div class="box-n state-list">
                <select id="mySelect" name="housing_update_statesList">
                    <option value="0">Select province</option>
                    <option value="Abia">Abia</option>
                    <option value="Adamawa">Adamawa</option>
                    <option value="Akwa Ibom">Akwa Ibom</option>
                    <option value="Anambra">Anambra</option>
                    <option value="Bauchi">Bauchi</option>
                    <option value="Bayelsa">Bayelsa</option>
                    <option value="Benue">Benue</option>
                    <option value="Borno">Borno</option>
                    <option value="Cross River">Cross River</option>
                    <option value="Delta">Delta</option>
                    <option value="Ebonyi">Ebonyi</option>
                    <option value="Edo">Edo</option>
                    <option value="Ekiti">Ekiti</option>
                    <option value="Enugu">Enugu</option>
                    <option value="Gombe">Gombe</option>
                    <option value="Imo">Imo</option>
                    <option value="Jigawa">Jigawa</option>
                    <option value="Kaduna">Kaduna</option>
                    <option value="Kano">Kano</option>
                    <option value="Katsina">Katsina</option>
                    <option value="Kebbi">Kebbi</option>
                    <option value="Kogi">Kogi</option>
                    <option value="Kwara">Kwara</option>
                    <option value="Lagos">Lagos</option>
                    <option value="Nasarawa">Nasarawa</option>
                    <option value="Niger">Niger</option>
                    <option value="Ogun">Ogun</option>
                    <option value="Ondo">Ondo</option>
                    <option value="Osun">Osun</option>
                    <option value="Oyo">Oyo</option>
                    <option value="Plateau">Plateau</option>
                    <option value="Rivers">Rivers</option>
                    <option value="Sokoto">Sokoto</option>
                    <option value="Taraba">Taraba</option>
                    <option value="Yobe">Yobe</option>
                    <option value="Zamfara">Zamfara</option>
                    <option value="FCT Abuja">FCT Abuja</option>
                </select>

                <script type="text/javascript" src="${request.static_url('spacenetng:static/tail.select/js/tail.select.js')}"></script>
                <!-- // This script below is used to customise the default html select dropdown appearance. -->
                <script type="text/javascript">
                    document.addEventListener("DOMContentLoaded", () => {
                        tail.select("select", {search: true, width: 200,});
                    });
                </script>
            </div>
            
            <div class="box-n product-description">
                <label for="textArea">
                <span for="description" style="font-size: 20px;">Product/Service description</span>
                <textarea name="housing_update_textarea" rows="10" cols="110" maxlength="186" id="textArea" placeholder="Write the description of your product/service here." required></textarea>                
                </label>
                <br/>

                Upload product images:
                <label>
                <input type="file" id="files" name="housing_update_images" multiple accept=".jpg, .jpeg, .png, .gif" required>
                </label>
                <div id="selectedFiles"></div>
            </div>
            <br>
            <br>

            <span style="font-size: 16px;">
                Your Product's category:
            </span>
            <div class=" box-n category-list">
              <label for="category1" class="container-radio2">Consumer Electronics
              <input type="radio" id="category1"
               name="housing_update_category" value="electronics" disabled>
              <span class="checkmark-radio2"></span>
              </label>
              <br>

              <label for="category2" class="container-radio2">Automobiles & Motorcycles
              <input type="radio" id="category2"
               name="housing_update_category" value="automobiles" disabled>
              <span class="checkmark-radio2"></span>
              </label>
              <br>

              <label for="category3" class="container-radio2">Housing, Events & Furnitures
              <input type="radio" id="category3"
               name="housing_update_category" value="housing" checked>
              <span class="checkmark-radio2"></span>
              </label>
              <br>

              <label for="category4" class="container-radio2">Fashion & Accessories
              <input type="radio" id="category4"
              name="housing_update_category" value="fashion" disabled>
              <span class="checkmark-radio2"></span>
              </label>
            </div>
            <br>

            <div class="box-n contact-details">
                <fieldset>
                    <legend>Contact Details</legend>
                    <!-- Code to match an email address -->
                    <!-- pattern="/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/" -->
                    <label> <span class="contacta-z">Individual/Company name:</span>
                        <input type="text" name="housing_update_username" maxlength="45" required>
                    </label>
                    <br>

                    <label> <span class="contacta-z">Email:</span>
                        <input type="email" name="housing_update_email" maxlength="40" required>
                    </label>
                    <br>
                    <br>

                    <label> <span class="contacta-z">Mobile number:</span>
                        <!-- Enter only digits from '0' to '9' -->
                        <input type="text" pattern="[0-9]+" name="housing_update_mobile" maxlength="15" required>
                    </label>
                    <br>
                    <br>

                    <label> <span class="contacta-z">Town/City:</span>
                        <!--Using regular expression for matching purposes.-->
                        <!-- Enter town/city name (upper and lowercase letters) -->
                        <input type="text" pattern="[A-Za-z' -]+" name="housing_update_city" maxlength="20" required>
                    </label>

                </fieldset>
            </div>
            <br>


            <span style="font-size: 16px;">
                Item's condition:
            </span>
            <div class=" box-n itemConditions">
              <label for="condition1" class="container-radio">New
              <input type="radio" id="condition1"
               name="housing_update_condition" value="New">
              <span class="checkmark-radio"></span>
              </label>
              <br>

              <label for="condition2" class="container-radio">Foreign Used
              <input type="radio" id="condition2"
               name="housing_update_condition" value="Foreign used">
              <span class="checkmark-radio"></span>
              </label>
              <br>

              <label for="condition3" class="container-radio">Locally Used
              <input type="radio" id="condition3"
               name="housing_update_condition" value="Locally used">
              <span class="checkmark-radio"></span>
              </label>
              <br>

              <label for="condition4A" class="container-radio">N/A
              <input type="radio" id="condition4A"
              name="housing_update_condition" value="NA" checked>
              <span class="checkmark-radio"></span>
              </label>
            </div>

            <div class="box-n terms-conditions">
                <script type="text/javascript" src="${request.static_url('spacenetng:static/jSfiles/embededScript1.js')}"></script>
                <label for="checkbox1" class="container-checkbox">
                    <input type="checkbox" name="checkbox" id="checkbox1" class="termscon" required onclick="mouseClick1()">
                    Agree to our terms and conditions.
                    <span class="checkmark-checkbox"></span>
                </label>
            </div>
            <br>
            
            <div id="create">
                <label for="final-submit">
                    <input type="submit" value="Update" id="final-submit" name="housing_update_submitButton" >
                </label>
                <br/>
                <span id="valid-region" hidden style="font-size: 18px; color: red;">Please select a valid region</span>
            </div>            
            </form>        
        </div>
